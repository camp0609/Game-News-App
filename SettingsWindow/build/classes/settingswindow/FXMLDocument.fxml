<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="ancSettings" prefHeight="460.0" prefWidth="468.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="settingswindow.FXMLDocumentController">
   <children>
      <Button fx:id="btnAddGame" layoutX="313.0" layoutY="113.0" mnemonicParsing="false" onMouseClicked="#handleAddButtonClicked" prefHeight="35.0" prefWidth="106.0" text="Add" />
      <TextField fx:id="txfAddGame" layoutX="44.0" layoutY="71.0" prefHeight="25.0" prefWidth="210.0" promptText="Add Game..." />
      <Button fx:id="btnOK" layoutX="44.0" layoutY="411.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" text="OK" />
      <Button fx:id="btnCancel" cancelButton="true" layoutX="202.0" layoutY="411.0" mnemonicParsing="false" onMouseClicked="#handleCancelButtonClicked" text="Cancel" />
      <ListView fx:id="lvGames" layoutX="44.0" layoutY="113.0" onMouseClicked="#handleGameSelected" prefHeight="262.0" prefWidth="210.0" />
      <Button fx:id="btnRemove" disable="true" layoutX="313.0" layoutY="266.0" mnemonicParsing="false" onMouseClicked="#handleRemoveButtonClicked" prefHeight="35.0" prefWidth="106.0" text="Remove" />
      <Button fx:id="btnRemoveAll" layoutX="313.0" layoutY="340.0" mnemonicParsing="false" onMouseClicked="#handleRemoveAllButtonClicked" prefHeight="35.0" prefWidth="106.0" text="Remove All" />
      <Button fx:id="btnEdit" layoutX="313.0" layoutY="195.0" mnemonicParsing="false" onMouseClicked="#handleEditButtonClicked" prefHeight="35.0" prefWidth="106.0" text="Edit" />
      <Label layoutX="193.0" layoutY="25.0" prefHeight="34.0" prefWidth="122.0" text="Add Games">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
